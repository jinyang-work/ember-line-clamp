  "keywords": [
    "ember-addon",
    "components",
    "truncate",
    "clamp",
    "overflow",
    "ellipsis",
    "see-more",
    "read-more"
  ],
  "license": "MIT",
  "author": "Luis Torres",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/Luinova/ember-line-clamp",
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint ./*.js addon addon-test-support app config lib server test-support tests",
    "start": "ember server",
    "test": "ember try:each"
  },
  "dependencies": {
    "ember-batcher": "^3.0.0",
    "ember-cli-babel": "^7.13.2",
    "ember-cli-htmlbars": "^4.2.2",
